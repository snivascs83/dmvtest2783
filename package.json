{
  "name": "dmv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ENV_TYPE=dev node operations.js && cross-env NODE_OPTIONS='--inspect' next",
    "build-preprod": "cross-env ENV_TYPE=preprod node operations.js && next build",
    "build-prod": "cross-env ENV_TYPE=prod node operations.js && next build",
    "build-uat": "cross-env ENV_TYPE=uat node operations.js && next build",
    "build-dev": "cross-env ENV_TYPE=dev node operations.js && next build",
    "build": "cross-env ENV_TYPE=dev node operations.js && next build",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "format:check": "prettier -c .",
    "test": "jest --passWithNoTests",
    "format": "prettier --write ./",
    "type-check": "tsc",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint ./src --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build",
    "prepare": "husky install",
    "pre-commit": "lint-staged --allow-empty --concurrent false",
    "check-sizes": "bundlesize"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.1.1",
    "@material-tailwind/react": "^2.1.8",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/axios": "^0.14.0",
    "@types/react-redux": "^7.1.33",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "heroicons-react": "^1.4.1",
    "lucide-react": "^0.297.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "react-select": "^5.8.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "sass": "^1.69.7",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^2.1.0",
    "universal-cookie": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-css-modules-transform": "^4.2.0",
    "ts-jest": "^27.0.5",
    "@types/jest": "^27.0.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "^20.9.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
